        <script type="text/javascript">
        function postToFeed() {
            ga('send', 'event', '15_napos_gombsor', 'facebook');
            var obj = {
              method: 'feed',
              redirect_uri: 'http://koponyeg.hu/?a=close_share',
              link: 'http://koponyeg.hu/t/Pilisborosjen%C5%91#shared_on_2016-02-22',
              picture: 'http://koponyeg.hu/?a=five_day_dia&r=1&d=2016-02-22',
              name: 'Pilisborosjenő időjárása | köpönyeg.hu',
              caption: '15 napos időjárás-előrejelzés',
              description: 'Várható időjárás: 2016. február 22. hétfő - Napos, gyengén felhős időnk lesz. Csak a fátyol- illetve gomolyfelhők szűrhetik a februári napsütést, melyekből csapadék sehol sem fordul elő.'
            };

            var callback = function(response) {
              document.getElementById('msg').innerHTML = "Post ID: " + response['post_id'];
            };

            FB.ui(obj, callback);
        }

        function openSharePopup(url, width, height, name) {
            width  = width || 575;
            height = height || 400;
            name = name || "share";
            var left   = ($(window).width()  - width)  / 2,
                top    = ($(window).height() - height) / 2,
                opts   = 'status=1' +
                         ',width='  + width  +
                         ',height=' + height +
                         ',top='    + top    +
                         ',left='   + left;
            window.open(url, name, opts);
        }

        function postToTwitter() {
            ga('send', 'event', '15_napos_gombsor', 'twitter');
            var url = "https://twitter.com/share";
            url += "?url=" + encodeURIComponent('http://koponyeg.hu/t/Pilisborosjen%C5%91#shared_on_2016-02-22');
            url += "&lang=hu";
            url += "&text=" + encodeURIComponent('Pilisborosjenő időjárása | köpönyeg.hu');
            url += "&related=" + encodeURIComponent('koponyeg:Köpönyeg.hu');
            openSharePopup(url, 575, 400);
        }

        function postToGooglePlus() {
            ga('send', 'event', '15_napos_gombsor', 'google_plus');
            var url = "https://plus.google.com/share";
            url += "?url=" + encodeURIComponent('http://koponyeg.hu/t/Pilisborosjen%C5%91#shared_on_2016-02-22');
            url += "&hl=hu";
            openSharePopup(url, 600, 600);
        }

    </script>

