    <script type="text/javascript">
    $(document).ready(function() {
	setTimeout(function() {
	
        $('.banner_datas').each(function() {
        	var $zonemain = $(this);
            $zonemain.children('.banner_html_code').each(function() {
            	var $elm = $(this);
				var scriptcode = $elm.attr('data-script');
				var getHtmlBanner = $elm.val();
                getHtmlBanner = getHtmlBanner.replace("<![CDATA[", "").replace("]]>", "");
				$elm.html(''); // delete content
	            //$(getHtmlBanner).appendTo('#' + $zonemain.attr('data-zone'));
                if($('#' + $zonemain.attr('data-zone')).length>0) {
                    postscribe('#' + $zonemain.attr('data-zone'), getHtmlBanner, {
                        done: function() {
                            if(scriptcode.length>0) {
                                eval(scriptcode);
                            }
                        }
                    });
                }
            });
        });
        $('#goascript').append('<scr' + 'ipt src="//ad.adverticum.net/g3.js"></scr' + 'ipt>');

        function trackAdBlocking() {
            if($(".adsbygoogle").css('display') == 'none') {
                ga('send', 'event', 'ad_block', 'used');
            } else {
                ga('send', 'event', 'ad_block', 'not_used');
            }
        }
        trackAdBlocking();
		
	}, 100);
    });
    </script>
    
    

    

